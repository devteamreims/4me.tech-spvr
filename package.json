    "colors": "1.1.2",
    "copy-webpack-plugin": "2.1.3",
    "css-loader": "0.23.1",
    "eslint": "2.8.0",
    "eslint-plugin-react": "5.0.1",
    "extract-text-webpack-plugin": "1.0.1",
